    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "babel-jest": "^24.9.0",
    "del-cli": "^3.0.0",
    "jest": "^24.9.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "files": [
    "lib"
